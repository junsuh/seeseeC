#include <stdio.h>

int main(){
	int a,b,c;

	for(a=0;a<10;a++){
		printf("                     ");
		for(b=10;b>a;b--){
			printf(" ");
		
		}
		for(c=0;c<2*a+1;c++){
			printf("*");
		}
		printf("\n");
	}

	for(a=33;a>27;a--){
		for(b=33;b>a;b--){
			printf(" ");
		}
		for(c=0;c<2*a-1;c++){
			printf("*");
	
		}
		printf("\n");
	}
	for(a=27;a<33;a++){
			for(b=32;b>a;b--){
				printf(" ");
		
			}
			for(c=0;c<2*a+1;c++){
				printf("*");
			}
			printf("\n");
		}

	for(a=10;a>0;a--){
		printf("                      ");
		for(b=10;b>a;b--){
			printf(" ");
		}
		for(c=0;c<2*a-1;c++){
			printf("*");
			
		}
		printf("\n");
	}

while(1);
return 0;
}
